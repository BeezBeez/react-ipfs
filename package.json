{
  "version": "0.2.46",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run doc && tsdx build",
    "size": "size-limit",
    "doc": "./node_modules/.bin/typedoc --out docs src --exclude index.ts",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-ipfs",
  "author": "Roman Yankowski",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youaresoroman/react-ipfs.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "User Interface",
    "Storybook",
    "IPFS"
  ],
  "bugs": {
    "url": "https://github.com/youaresoroman/react-ipfs/issues"
  },
  "homepage": "https://github.com/youaresoroman/react-ipfs#readme",
  "module": "dist/react-ipfs.esm.js",
  "size-limit": [
    {
      "path": "dist/react-ipfs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-ipfs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "ipfs": "^0.58.4",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-scss": "^3.0.0",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.2",
    "uint8arrays": "^3.0.0"
  },
  "dependencies": {
    "react-context-hook": "^3.0.7"
  }
}
